algoritmo "DOBRO_TERÇO"
//Algoritmo que dobra e mostra a terça parte do dado fornecido pelo usuário.
var
   x,y,t:real
inicio
//Entrada de dados
   escreva("Digite um número: ")
   leia(x)
//Equação do dobro e da terça parte.
   y <- x*2
   t <- x/3
//Saída de dados
   escreval("O dobro do número informado é ", y)
   escreva("A terça parte do número informado é ", t:7:5)

fimalgoritmo