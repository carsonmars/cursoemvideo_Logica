algoritmo "DELTA"
var
   a,b,c,delta:real
inicio
//Entrada de dados
   escreva("Digite o valor de a: ")
   leia(a)
   escreva("Digite o valor de b: ")
   leia(b)
   escreva("Digite o valor de c: ")
   leia(c)
//Equação do delta
   delta <- b^2-4*a*c
//Saída de dados
   escreval
   escreva("Delta é ", delta,".")
   escreval
fimalgoritmo