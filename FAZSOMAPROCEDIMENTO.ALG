algoritmo "fazSomaProcedimento"

var
 X, Y:inteiro
procedimento Soma(A, B:inteiro)
   inicio
      escreval("Recebido o valor: ",A)
      escreval("Recebido o valor: ",B)
      escreval
      escreval("A soma entre os valores é: ", A + B)
   fimprocedimento
inicio
   X <- 5
   Y <- 3
   Soma(X,Y)
fimalgoritmo